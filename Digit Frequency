
#include <stdio.h>

int main() {
    long long num;
    int freq[10] = {0};  

    printf("Enter a number: ");
    scanf("%lld", &num);

  
    if (num < 0) num = -num;

    while (num != 0) {
        int digit = num % 10;
        freq[digit]++;
        num /= 10;
    }

    printf("\nDigit Frequency:\n");
    for (int i = 0; i < 10; i++) {
        if (freq[i] > 0)
            printf("Digit %d: %d times\n", i, freq[i]);
    }

    return 0;
}
